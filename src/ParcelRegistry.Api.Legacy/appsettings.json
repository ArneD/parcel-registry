{
  "ConnectionStrings": {
    "Events": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.ParcelRegistry;Trusted_Connection=True;",
    "LegacyProjections": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.ParcelRegistry;Trusted_Connection=True;",
    "LegacyProjectionsAdmin": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.ParcelRegistry;Trusted_Connection=True;"
  },

  "DataDog": {
    "Enabled": true,
    "Debug": false,
    "ServiceName": "parcel-registry-legacy-api"
  },

  "Cors": [
    "http://localhost:3000",
    "http://localhost:5000"
  ],

  "Naamruimte": "https://data.vlaanderen.be/id/perceel",
  "DetailUrl": "https://basisregisters.vlaanderen.be/api/v1/percelen/{0}",
  "VolgendeUrl": "https://basisregisters.vlaanderen.be/api/v1/percelen?offset={0}&limit={1}",
  "AdresDetailUrl": "https://basisregisters.vlaanderen.be/api/v1/adressen/{0}",

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "ParcelRegistry": "Debug",
      "Aiv": "Debug",
      "Be.Vlaanderen.Basisregisters.DataDog.Tracing": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "ParcelRegistry": "Verbose",
        "Aiv": "Verbose",
        "Be.Vlaanderen.Basisregisters.DataDog.Tracing": "Information",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Properties": {
      "Application": "ParcelRegistry - Legacy API"
    }
  }
}
