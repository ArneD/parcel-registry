{
  "ConnectionStrings": {
    "SyndicationProjections": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.ParcelRegistry;Trusted_Connection=True;",
    "SyndicationProjectionsAdmin": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.ParcelRegistry;Trusted_Connection=True;"
  },

  "DataDog": {
    "Enabled": false,
    "Debug": false
  },

  "SyndicationFeeds": {
    "Address": "https://legacy.adres.staging-basisregisters.vlaanderen/v1/adressen/sync?embed=true",
    "AddressAuthUserName": "x",
    "AddressAuthPassword": "x",
    "AddressPollingInMilliseconds": 5000
  },

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "ParcelRegistry": "Information",
      "Be.Vlaanderen.Basisregisters": "Debug",
      "Be.Vlaanderen.Basisregisters.DataDog.Tracing": "Debug",
      "Be.Vlaanderen.Basisregisters.AspNetCore.Mvc.Logging": "Information",
      "Marvin": "Debug",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "ParcelRegistry": "Debug",
        "Be.Vlaanderen.Basisregisters": "Verbose",
        "Be.Vlaanderen.Basisregisters.DataDog.Tracing": "Verbose",
        "Be.Vlaanderen.Basisregisters.AspNetCore.Mvc.Logging": "Error",
        "Marvin": "Verbose",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Properties": {
      "Application": "ParcelRegistry - Syndication Projections",
      "ContainerId": "REPLACE_CONTAINERID"
    }
  }
}
